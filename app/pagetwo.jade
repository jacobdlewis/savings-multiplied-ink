html(lang="en-US")
	head
		meta(charset="utf-8")
		meta(http-equiv="X-UA-Compatible", content="IE=edge")
		meta(name="viewport", content="width=device-width, initial-scale=1")
		title Products
		link(rel="stylesheet" href="/vendor/ink/dist/css/ink-flex.min.css")
		link(rel="stylesheet" href="css/main.css")
		//- Ink-grid is manditory for colums to work
		.ink-grid
			.column-group.row-1
				.all-70.row-1-col-1
				.all-30.row-1-col-2
					.login
						p sign in | sign up

			.column-group.row-2
				.all-10.col-1
				.column-group.all-80.col-2
						main-logo.xlarge-50.large-60.medium-40.small-100.tiny-100
							img.main-logo(src="https://raw.githubusercontent.com/elizabrock/SavingsMultipliedRedux/master/app/assets/images/savings-multiplied-logo.png")
						.main-header-nav.xlarge-50.large-40.medium-20.small-100.tiny-100
							ul
								li.about About
								li.content Content
								li.blog Blog
								li.press Press
								img.main-tagline(src="https://raw.githubusercontent.com/elizabrock/SavingsMultipliedRedux/master/app/assets/images/tagline.png")
				.all-10.col-4

			.column-group.row-3
				.middle-nav.xlarge-100.large-100.medium-50.small-50.tiny-10
					ul.middle-nav
						li.buy Buy
						li.sell Sell
						li.how How it Works
						li.links Links
						li.community Community
						input(type="text", placeholder="Search")

			.column-group.row-2.main-wrapper-group.pagetwo
				.all-10.col-1.main__side
				.column-group.all-80.col-5
					.side-nav.all-15.pagetwo
						h4.side-nav__title Limit your search by:
						.ink-form
							.control-group.append-button.side-nav__search
								input(type="text")
								button.ink-button Search
						.column-group
							.all-50
								p.side-nav__p-top Condition:
							.all-50
								<input type="checkbox" name="size" value="New"/> New <br>
								<input type="checkbox" name="size" value="Gently Used"/> Gently Used
						p.side-nav__p Size:
						.column-group
							.all-20
							form.all-10
								<input type="checkbox" name="size" value="2"/> 2
								<input type="checkbox" name="size" value="3"/> 3
								<input type="checkbox" name="size" value="4"/> 4
								<input type="checkbox" name="size" value="5"/> 5
							.all-25
							form.all-10
								<input type="checkbox" name="size" value="6"/> 6
								<input type="checkbox" name="size" value="7"/> 7
								<input type="checkbox" name="size" value="8"/> 8
								<input type="checkbox" name="size" value="9"/> 9
							.all-25
						p.side-nav__p Season:
						.column-group
							.all-20
							form.all-60
								<input type="checkbox" name="season" value="All"/> All Seasons <br>
								<input type="checkbox" name="season" value="Spring"/> Spring <br>
								<input type="checkbox" name="season" value="Summer"/> Summer <br>
								<input type="checkbox" name="season" value="Fall"/> Fall <br>
								<input type="checkbox" name="season" value="Winter"/> Winter <br>
							.all-20
						p.side-nav__p Type:
						.column-group
							.all-20
							form.all-60
								<input type="checkbox" name="type" value="Any"/> Any <br>
								<input type="checkbox" name="type" value="One"/> One-Piece <br>
								<input type="checkbox" name="type" value="Tops"/> Tops <br>
								<input type="checkbox" name="type" value="Bottoms"/> Bottoms <br>
							.all-20
						p.side-nav__p Brand:
						.column-group
							.all-20
							.all-60
								select
									option(value="One") One
									option(value="Two") Two
									option(value="Three") Three
									option(value="Four") Four
							.all-20


